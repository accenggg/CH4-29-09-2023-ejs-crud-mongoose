{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/dashboard/add",
      "dest": "/server.js"
    },
    {
      "src": "/dashboard/edit/:id",
      "dest": "/server.js"
    },
    {
      "src": "/dashboard/delete/:id",
      "dest": "/server.js"
    },
    {
      "src": "/dashboard/update/:id",
      "dest": "/server.js"
    },
    {
      "src": "/dashboard",
      "dest": "/server.js"
    },
    {
      "src": "/api/v1/tours",
      "dest": "/server.js"
    },
    {
      "src": "/api/v1/users",
      "dest": "/server.js"
    }
  ]
}
